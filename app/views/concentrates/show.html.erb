<div class="row">
	<div class="large-10 large-2-offset columns">
		<h3><%=@concentrate.strain%>
			<small> concentrate
				<% if current_user && current_user.admin? %>
					<%=link_to('edit', edit_merchant_concentrate_path(Merchant.find(params[:merchant_id]), @concentrate))%>
				<% end %>
			</small>
		</h3>
		<div class="panel">
			<span>Price: <%=@concentrate.quantity_price%></span> <span> Fees: <%=@concentrate.fees%></span>
			<span class="right"><%=@concentrate.kind%></span>
		</div>
		<%= render 'partials/mantis_ad_network' %>
	</div>
</div>
