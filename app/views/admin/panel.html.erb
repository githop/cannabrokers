<div class="row">
	<div class="large-11 large-offset-1 columns">
		<h1 class="text-center">Admin Panel</h1>

		<h3 class="text-center"><%=link_to("Add a new Flower", new_flower_path)%></h3>
		<div class="row">
			<div class="large-6 columns">
				<h3>Add a new Concentrate <small>Pick a company</small></h3>
				<ul>
				<% @concentrates.each do |merch| %>
					<li><%=link_to(merch.name, new_merchant_concentrate_path(merch))%></li>
				<% end %>
				</ul>
			</div>
			<div class="large-6 columns">
			<h3>Edible Companies <small>Pick a company</small></h3>
			<ul>
			<% @edibles.each do |merch| %>
				<li><%= link_to(merch.name, new_merchant_edible_path(merch))%></li>
			<% end %>
			</ul>
			</div>
		</div>
		
	
	</div>
</div>