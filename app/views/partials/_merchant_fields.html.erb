<%=form_for @merchant do |f| %>
			<p>
				<%=f.label :name %>
				<%=f.text_field :name %>
			</p>

			<p>
				<%=f.label :description %>
				<%=f.text_area :description %>
			</p>

			<p>
				<%=f.label :phone %>
				<%=f.phone_field :phone %>
			</p>

			<p>
				<%=f.label :premium_merchant? %>
				<%=f.check_box :premium %>
			</p>

			<p>
				<%=f.submit class: "button"%>
			</p>
		<% end %>